{
    "name": "janwebdev/translatable-entity-bundle",
    "description": "Make Entity translatable",
    "keywords": ["symfony i18n translation"],
    "type": "symfony-bundle",
    "homepage": "https://github.com/janwebdev/translatable-entity-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Jan Rogozinsky",
            "email": "janwebdev@gmail.com"
        }
    ],
    "require":{
        "php": ">=7.4",
        "symfony/config": "^5.3",
        "symfony/dependency-injection": "^5.3",
        "symfony/framework-bundle": "^5.3",
        "doctrine/orm": "^2.5"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "symfony/test-pack": "*",
        "phpunit/phpunit": "^8.0",
        "symfony/yaml": "^5.3"
    },
    "autoload": {
        "psr-4": {
            "Janwebdev\\TranslatableEntityBundle\\":  "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Janwebdev\\TranslatableEntityBundle\\Tests\\":  "tests/"
        }
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": "true",
            "require": "^5.3"
        }
    }
}
